    "react-select": "^2.4.3",
    "react-switch": "^4.1.0",
    "react-toast-notifications": "^1.4.0",
    "recharts": "^1.5.0",
    "secp256k1": "^3.6.2",
    "sleep-promise": "^8.0.1",
    "sticky-events": "^2.4.0",
    "store": "^2.0.12",
    "styled-components": "^4.2.0",
    "threads": "^0.12.1",
    "web3": "^1.0.0-beta.54"
  }
}
