{
  "name": "d.wallet",
  "version": "0.1.3",
  "description": "A Decentralized Cryptocurrency Wallet",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "lint": "tslint '{src,test,mocks}/**/*.{ts,tsx}' --project ./tsconfig.json",
    "precommit": "lint-staged",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{js,ts,tsx,json,css,scss,md}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "tslint --project ./tsconfig.json --fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    }
  },
  "build": {
    "productName": "d.Wallet",
    "appId": "io.dwallet",
    "mac": {
      "category": "business",
      "icon": "./icons/wallet.icns"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json",
      "icons/"
    ],
    "linux": {
      "target": "deb",
      "icon": "./icons/wallet.png"
    },
    "win": {
      "target": "nsis",
      "icon": "./icons/wallet.png"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Robinfr/electron-react-typescript.git"
  },
  "author": {
    "name": "d.Wallet",
    "email": "walletbank@protonmail.com"
  },
  "license": "SEE LICENSE",
  "bugs": {
    "url": "https://github.com/Robinfr/electron-react-typescript/issues"
  },
  "homepage": "https://github.com/Robinfr/electron-react-typescript",
  "devDependencies": {
    "@types/animejs": "^2.0.2",
    "@types/bitcore-lib": "^0.15.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/ethereumjs-tx": "^1.0.1",
    "@types/jest": "^21.1.10",
    "@types/qrcode.react": "^0.8.2",
    "@types/react": "^16.8.15",
    "@types/react-countup": "^4.0.1",
    "@types/react-dom": "^16.8.4",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-redux": "^6.0.14",
    "@types/react-router-dom": "^4.3.2",
    "@types/react-select": "^2.0.17",
    "@types/react-test-renderer": "^16.8.1",
    "@types/secp256k1": "^3.5.0",
    "@types/store": "^2.0.1",
    "@types/webdriverio": "^4.13.3",
    "@types/webpack-env": "^1.13.9",
    "awesome-typescript-loader": "^3.5.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.11",
    "electron": "^4.1.5",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.6.0",
    "jest": "^23.6.0",
    "lint-staged": "^6.1.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.0",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^6.0.7",
