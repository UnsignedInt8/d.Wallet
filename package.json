    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.4.5",
    "uglify-js": "^3.5.10",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.5",
    "webpack-merge": "^4.2.1"
  },
